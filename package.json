{
  "name": "fichaz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 next dev --turbopack --experimental-https",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint src scripts",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "clean": "rm -rf .next out dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run --reporter=verbose --testNamePattern=\"accessibility|a11y\"",
    "check:no-secrets-logged": "node ./scripts/check-no-secrets-logged.js",
    "pre-commit": "pnpm format && pnpm check:no-secrets-logged && pnpm lint && pnpm type-check && pnpm test && pnpm test:a11y",
    "validate": "pnpm pre-commit && pnpm build",
    "quick-check": "pnpm type-check && pnpm lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "generate:page": "tsx scripts/generate-page.ts",
    "generate:from-config": "tsx scripts/generators/api/generate-from-config.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kinde-oss/kinde-auth-nextjs": "^2.9.1",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/og": "^0.8.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.541.0",
    "nanoid": "^5.1.6",
    "next": "15.5.0",
    "next-intl": "^4.3.5",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@clack/prompts": "^0.11.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "axe-core": "^4.10.3",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "happy-dom": "^20.0.2",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "msw": "^2.10.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typedoc": "^0.28.10",
    "typescript": "^5",
    "vercel": "^46.0.2",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
